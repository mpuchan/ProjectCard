<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Position</th>
      <th>Board</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= list.position %></td>
        <td><%= list.board_id %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List', new_board_list_path %>
